{
  "manifest_version": 3,
  "name": "AI Studio Enchanted Queue",
  "version": "1.0",
  "description": "Adds a smart message queue to Google AI Studio.",
  "host_permissions": [
    "https://aistudio.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://aistudio.google.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}